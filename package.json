{
  "name": "remembered_app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fortawesome/fontawesome-free": "^6.2.1",
    "@mui/joy": "^5.0.0-alpha.59",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "adm-zip": "^0.5.9",
    "build-electron": "^1.0.5",
    "bulma": "^0.9.4",
    "concurrently": "^7.6.0",
    "electron-is-dev": "^2.0.0",
    "esm": "^3.2.25",
    "get-folder-size": "^4.0.0",
    "junk": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-modal": "^3.16.1",
    "react-router-dom": "^6.5.0",
    "react-scripts": "^5.0.1",
    "sqlite3": "^5.1.4",
    "utf8": "^3.0.0",
    "wait-on": "^7.0.1",
    "web-vitals": "^2.1.4"
  },
  "main": "public/main.js",
  "build": {
    "extraMetadata": {
      "main": "build/main.js"
    },
    "files": [
      "build/**/*"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "build-electron && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron-react": "concurrently -k \"BROWSER=none react-scripts start\" \"build-electron -d\" \"wait-on public/.build-electron-done http://localhost:3000 && electron --trace-warnings .\"",
    "postinstall": "electron-builder install-app-deps"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "^22.0.0",
    "electron-builder": "^23.6.0"
  }
}
